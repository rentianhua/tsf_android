<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f0eff5"
    tools:context="com.dumu.housego.BlockTradeDetailActivity"
    tools:ignore="HardcodedText,RtlHardcoded,ContentDescription,UselessParent">

    <RelativeLayout
        android:id="@+id/rl_block_trade_title"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="#e12442">

        <ImageView
            android:id="@+id/iv_block_trade_detail_location"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="16dp"
            android:src="@drawable/location_white_40x31" />


        <LinearLayout
            android:id="@+id/ll_block_trade_detail_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="10dp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/imageView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:src="@drawable/white_left" />

        </LinearLayout>

        <TextView
            android:id="@+id/tv_block_trade_detail_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="三科麓湾"
            android:textColor="#ffffff" />

    </RelativeLayout>


    <com.dumu.housego.util.MyReboundScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/rl_block_trade_title">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <!-- 框內內容，scrollView -->

            <ImageView
                android:id="@+id/iv_block_trade_detail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal" />


            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#fff">

                <TextView
                    android:id="@+id/tv_block_trade_detail_content"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_alignParentTop="true"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:text="城建仁山智水花园" />


                <TextView
                    android:id="@+id/textView2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/tv_block_trade_detail_content"
                    android:layout_below="@+id/tv_block_trade_detail_content"
                    android:layout_marginTop="15dp"
                    android:text="勾地需付款:"
                    android:textColor="#999"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/TextView01"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/textView2"
                    android:layout_below="@+id/textView2"
                    android:layout_marginTop="10dp"
                    android:text="更新时间:"
                    android:textColor="#999"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/TextView031"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/TextView01"
                    android:layout_below="@+id/TextView01"
                    android:layout_marginTop="10dp"
                    android:text="房源编号:"
                    android:textColor="#333"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv_block_trade_detail_bianhao"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/TextView031"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_toRightOf="@+id/TextView031"
                    android:text="2016-09-21"
                    android:textColor="#333"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv_block_trade_detail_goudi"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBottom="@+id/textView2"
                    android:layout_marginLeft="10dp"
                    android:layout_toRightOf="@+id/textView2"
                    android:text="0.01"
                    android:textSize="14sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/tv_block_trade_detail_goudi"
                    android:layout_marginLeft="2dp"
                    android:layout_toRightOf="@+id/tv_block_trade_detail_goudi"
                    android:text="元"
                    android:textColor="#999"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv_block_trade_detail_date"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/TextView01"
                    android:layout_alignBottom="@+id/TextView01"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_toRightOf="@+id/TextView01"
                    android:text="2016-09-21"
                    android:textColor="#999"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv_block_trade_goudiimage"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/tv_block_trade_detail_date"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="10dp"
                    android:background="#E12442"
                    android:gravity="center"
                    android:paddingBottom="2dp"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:paddingTop="2dp"
                    android:text="勾地"
                    android:textColor="#ffffff"
                    android:textSize="14sp" />

            </RelativeLayout>

            <Button
                android:layout_width="match_parent"
                android:layout_height="10dp"
                android:background="#fff" />


            <Button
                android:layout_width="match_parent"
                android:layout_height="@dimen/fengexian"
                android:background="#c9c9cb" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:background="#fff"
                android:padding="10dp"
                android:text="基本信息"
                android:textColor="#333"
                android:textSize="13sp" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="@dimen/fengexian"
                android:background="#c9c9cb" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#fff">

                <TextView
                    android:id="@+id/textView1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="10dp"
                    android:text="所在地区:"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/textView1"
                    android:layout_below="@+id/textView1"
                    android:layout_marginTop="10dp"
                    android:text="合作方式:"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/textView4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/textView3"
                    android:layout_below="@+id/textView3"
                    android:layout_marginTop="10dp"
                    android:text="物业类型:"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv_block_trade_detail_area"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/textView3"
                    android:layout_toRightOf="@+id/textView1"
                    android:text="光明新区 光明"
                    android:textColor="#000000"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv_block_trade_detail_hezuofangshi"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_above="@+id/textView4"
                    android:layout_alignLeft="@+id/tv_block_trade_detail_area"
                    android:text="整体转让"
                    android:textColor="#000000"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv_block_trade_detail_wuyeleixing"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/textView4"
                    android:layout_alignBottom="@+id/textView4"
                    android:layout_toRightOf="@+id/textView4"
                    android:text="商业用房"
                    android:textColor="#000000"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/TextView03"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/tv_block_trade_detail_area"
                    android:layout_alignBottom="@+id/tv_block_trade_detail_area"
                    android:layout_marginLeft="19dp"
                    android:layout_toRightOf="@+id/tv_block_trade_detail_area"
                    android:text="预算金额:"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv_block_trade_detail_yusuanjine"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/TextView03"
                    android:layout_alignBottom="@+id/TextView03"
                    android:layout_toRightOf="@+id/TextView03"
                    android:text="400万"
                    android:textColor="#000000"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/TextView04"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/tv_block_trade_detail_hezuofangshi"
                    android:layout_alignBottom="@+id/tv_block_trade_detail_hezuofangshi"
                    android:layout_alignLeft="@+id/TextView03"
                    android:text="使用年限:"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv_block_trade_detail_shiyongnianxian"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/TextView04"
                    android:layout_alignBottom="@+id/TextView04"
                    android:layout_toRightOf="@+id/TextView04"
                    android:text="30年"
                    android:textColor="#000000"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/textView8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/textView4"
                    android:layout_below="@+id/textView4"
                    android:layout_marginTop="10dp"
                    android:text="土地属性:"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv_block_trade_detail_address"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/textView8"
                    android:layout_alignBottom="@+id/textView8"
                    android:layout_toRightOf="@+id/textView8"
                    android:text="葵涌街道喜用高速出口"
                    android:textColor="#000000"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/TextView05"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/textView8"
                    android:layout_below="@+id/textView8"
                    android:layout_marginTop="10dp"
                    android:text="建筑面积:"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv_block_trade_detail_jianzhumianji"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/TextView05"
                    android:layout_alignBottom="@+id/TextView05"
                    android:layout_toRightOf="@+id/TextView05"
                    android:text="200㎡"
                    android:textColor="#000000"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/textView7"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/TextView05"
                    android:layout_below="@+id/TextView05"
                    android:layout_marginTop="10dp"
                    android:text="联系人:"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv_block_trade_detail_lianxiren"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/textView7"
                    android:layout_alignBottom="@+id/textView7"
                    android:layout_toRightOf="@+id/textView7"
                    android:text=""
                    android:textColor="#000000"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/textView9"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignLeft="@+id/textView7"
                    android:layout_below="@+id/textView7"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:text="位置:"
                    android:textSize="12sp" />

                <TextView
                    android:id="@+id/tv_block_trade_detail_weizhi"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignBaseline="@+id/textView9"
                    android:layout_alignBottom="@+id/textView9"
                    android:layout_toRightOf="@+id/textView9"
                    android:text=""
                    android:textColor="#000000"
                    android:textSize="12sp" />
            </RelativeLayout>

            <Button
                android:layout_width="match_parent"
                android:layout_height="@dimen/fengexian"
                android:layout_marginTop="10dp"
                android:background="#c9c9cb" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#fff"
                android:padding="10dp"
                android:text="详细介绍"
                android:textColor="#333"
                android:textSize="13sp" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="@dimen/fengexian"
                android:background="#c9c9cb" />

            <WebView
                android:id="@+id/wb_bolck_content"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:minHeight="40dp" />


            <Button
                android:layout_width="match_parent"
                android:layout_height="@dimen/fengexian"
                android:layout_marginTop="10dp"
                android:background="#c9c9cb" />

            <LinearLayout
                android:id="@+id/nogoudi"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:visibility="gone">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="#fff">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_centerHorizontal="true"
                        android:gravity="center_vertical"
                        android:padding="10dp"
                        android:text="认证信息"
                        android:textColor="#333"
                        android:textSize="13sp" />
                </RelativeLayout>

                <WebView
                    android:id="@+id/renzhenxinxi"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:minHeight="40dp" />


                <Button
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/fengexian"
                    android:background="#c9c9cb" />

                <RelativeLayout
                    android:id="@+id/rl_block_map"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:background="#fff">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_centerHorizontal="true"
                        android:gravity="center_vertical"
                        android:padding="10dp"
                        android:text="位置及周边"
                        android:textColor="#333"
                        android:textSize="13sp" />

                    <ImageView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_centerVertical="true"
                        android:layout_margin="10dp"
                        android:src="@drawable/gray_right" />
                </RelativeLayout>

                <Button
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/fengexian"
                    android:background="#c9c9cb" />

                <com.baidu.mapapi.map.MapView
                    android:id="@+id/block_bmapView"
                    android:layout_width="match_parent"
                    android:layout_height="180dp"
                    android:clickable="false" />

            </LinearLayout>

            <Button
                android:layout_width="match_parent"
                android:layout_height="20dp"
                android:background="#f0eff5" />
            <!-- 框內內容，scrollView -->

        </LinearLayout>
    </com.dumu.housego.util.MyReboundScrollView>

</RelativeLayout>
