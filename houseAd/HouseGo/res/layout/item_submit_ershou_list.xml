<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:descendantFocusability="blocksDescendants"
    android:layout_width="match_parent"
    android:background="#f0eff5"
    android:layout_height="match_parent" >
    
    <RelativeLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:background="@drawable/btn_submit_shap2"
        >
    
 <RelativeLayout
       android:id="@+id/item_left"
        android:padding="5dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
          >
     	
     
     
     
    			
    			 
                    <ImageView
            android:id="@+id/iv_submit_ershou_list"
            android:layout_width="120dp"
            android:layout_height="80dp"
                        android:scaleType="centerCrop"
                        android:background="#ffaaaaaa"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="6dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="6dp"
            android:src="@drawable/touxiang3" />

            <com.dumu.housego.entity.MyTextView
                android:id="@+id/tv_submit_ershou_list_detail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/tv_submit_ershou_list_title"
                android:layout_below="@+id/tv_submit_ershou_list_title"
                android:layout_marginTop="2dp"
                android:paddingRight="40dp"
                android:singleLine="true"
                android:ellipsize="end"
                android:textColor="#999"
                android:text="龙岗新区/布吉/国展苑"
                android:textSize="12sp" />

            <com.dumu.housego.entity.MyTextView
                android:id="@+id/tv_submit_ershou_list_zongjia"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/tv_submit_ershou_list_detail"
                android:layout_toLeftOf="@+id/textView1"
                android:text="450"
                android:textColor="#ff0000"
                android:textSize="14sp" />

            <com.dumu.housego.entity.MyTextView
                android:id="@+id/tv_submit_ershou_list_louceng"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignLeft="@+id/tv_submit_ershou_list_detail"
                android:layout_below="@+id/tv_submit_ershou_list_detail"
                android:layout_marginTop="2dp"
                android:text="中层(共26层)"
                 android:textColor="#999"
                android:textSize="12sp" />

            <com.dumu.housego.entity.MyTextView
                android:id="@+id/tv_submit_ershou_list_chenhu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:maxLines="1"
                android:layout_alignLeft="@+id/tv_submit_ershou_list_louceng"
                android:layout_below="@+id/tv_submit_ershou_list_louceng"
                android:layout_marginTop="2dp"
                android:text="15577330719 / 自售"
                 android:textColor="#999"
                android:paddingBottom="6dp"
                android:textSize="12sp" />

            <com.dumu.housego.entity.MyTextView
                android:id="@+id/tv_submit_ershou_list_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_alignTop="@+id/iv_submit_ershou_list"
                android:layout_toRightOf="@+id/iv_submit_ershou_list"
                android:text="深圳大鹏新区葵涌"
                 android:singleLine="true"
                android:textSize="12sp" />

            <com.dumu.housego.entity.MyTextView
                android:id="@+id/textView1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/tv_submit_ershou_list_detail"
                android:layout_alignParentRight="true"
                android:layout_marginRight="5dp"
                android:text="万"
                android:textColor="#ff0000"
                android:textSize="14sp" />

            
             <Button
    			     android:id="@+id/line"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/fengexian"
                    android:layout_marginLeft="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginRight="5dp"
                    android:layout_below="@+id/iv_submit_ershou_list"
                    android:background="#c9c9cb" />
            
            <LinearLayout 
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_below="@+id/line"
                >
             
            <com.dumu.housego.entity.MyTextView
                android:id="@+id/tv_submit_ershou_list_hetong"
              android:layout_width="0dp"
              android:layout_weight="1"
              android:lines="1"
                android:layout_height="wrap_content"
                android:layout_below="@+id/iv_submit_ershou_list"
                android:layout_alignParentRight="true"
             android:layout_gravity="center_vertical"
                android:text="上传合同"
                android:gravity="center"
               android:layout_marginLeft="6dp"
               
                android:onClick="doClick"
                  android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
               android:textColor="#333"
                android:textSize="12sp" />
            
            <com.dumu.housego.entity.MyTextView
                android:id="@+id/tv_submit_ershou_list_sfz"
                android:layout_width="0dp"
              android:layout_weight="1"
              android:lines="1"
                android:gravity="center"
                android:layout_height="wrap_content"
                android:layout_alignBaseline="@+id/tv_submit_ershou_list_hetong"
                android:layout_toLeftOf="@+id/tv_submit_ershou_list_hetong"
                android:text="上传身份证"
                   android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:paddingLeft="5dp"
                 android:layout_marginLeft="6dp"
                android:paddingRight="5dp"
                  android:textColor="#333"
                 android:layout_gravity="center_vertical"
                  android:onClick="doClick"
                android:textSize="12sp" />
            
            
            <com.dumu.housego.entity.MyTextView 
                android:id="@+id/tv_shenqingshouchu"
                android:layout_width="0dp"
              android:layout_weight="1"
              android:lines="1"
                  android:gravity="center"
                android:layout_height="wrap_content"
                android:text="申请已售出"
                android:textSize="12sp"
                android:layout_gravity="center_vertical"
                android:paddingTop="10dp"
                android:paddingBottom="10dp"
                 android:layout_marginLeft="6dp"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                
                />

            <ImageView
                android:layout_marginLeft="6dp"
                android:id="@+id/iv_submit_ershou_list_edit"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                      android:layout_gravity="center_vertical"
                android:onClick="doClick"
                android:src="@drawable/bianjipinglun"
                   android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
                />
            
              <ImageView 
                     android:layout_marginLeft="6dp"
                  android:layout_marginRight="5dp"
 	       android:id="@+id/iv_submit_ershou_list_delete"
 	       android:layout_width="wrap_content"
 	          android:paddingTop="10dp"
                android:paddingBottom="10dp"
                android:paddingLeft="5dp"
                android:paddingRight="5dp"
 	      	android:layout_centerInParent="true"
 	       android:layout_height="wrap_content"
 	       android:layout_gravity="center"
 	       android:src="@drawable/delete_iv"
 	       />
</LinearLayout>
        </RelativeLayout>
    </RelativeLayout>

</RelativeLayout>
